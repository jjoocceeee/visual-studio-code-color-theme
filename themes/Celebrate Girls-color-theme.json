	//THeme colors 
	// Teal: 236476
	// Light Blue: 68B8C1
	// Light Gray: D2D2D0
	// Deep Pink: A94382
	// Light Pink: FB90A2
	// Purple: 5F3865
	// Yellow: E3C07C
	// Salmon: F78C6C
	// Green: 7DAD3F (comments)
	//JavaScript
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#236476",	//Teal
			},
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#E0B4C3"	//Light Pink
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Meta Literal",
			"scope": [
				"meta.object-literal",
				"met.object.member",
				"meta.block",
				"support.type.primitive"
			],
			"settings": {
				"foreground": "#E3C07C"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"keyword.operator",
				"punctuation",
				"punctuation.definition.tag",
